<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:text="You shouldn't see this..."
        android:textSize="24sp"
        android:textColor="#FF303F9F"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="57dp"
        android:id="@+id/result_title"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_width="300dp" />

    <TextView
        android:text="units"
        android:textSize="22sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:layout_alignBaseline="@+id/shoot_up_tb"
        android:layout_alignBottom="@+id/shoot_up_tb"
        android:layout_toEndOf="@+id/shoot_up_tb" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="normal"
        app:srcCompat="@drawable/ic_back_fob_img"
        android:layout_marginLeft="@dimen/fab_margin"
        android:layout_marginRight="@dimen/fab_margin"
        android:layout_marginStart="21dp"
        android:layout_marginBottom="16dp"
        android:id="@+id/im_done_fab"
        android:layout_alignParentBottom="true"
        app:backgroundTint="@color/colorPrimaryDark"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Enter Insulin Injected: "
        android:textSize="24sp"
        android:textColor="#FF303F9F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/result_title_supp"
        android:layout_below="@+id/result_title"
        android:layout_alignStart="@+id/result_title"
        android:layout_marginTop="44dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/shoot_up_tb"
        android:hint="Dose..."
        android:layout_marginTop="35dp"
        android:layout_below="@+id/result_title_supp"
        android:layout_alignEnd="@+id/result_title_supp" />


</RelativeLayout>